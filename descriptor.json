{
  "name": "Exportation",
  "description": "Plugin to export information from detections and manual annotations included in a patch",
  "container-image": {
    "image": "cellsia/s_exportation",
    "type": "singularity"
  },
  "command-line": "[CYTOMINE_HOST] [CYTOMINE_PUBLIC_KEY] [CYTOMINE_PRIVATE_KEY] [CYTOMINE_ID_PROJECT] [CYTOMINE_ID_SOFTWARE] [IMAGE_TO_ANALYZE] [OFFSET] [BOX_SIZE] [TRAINING_TYPE] [OUTPUT_FILE]",
  "inputs": [
    {
      "id": "cytomine_host",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine host",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_public_key",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine public key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_private_key",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine private key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_id_project",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine project ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "cytomine_id_software",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine software ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "image_to_analyze",
      "description": "Image with patches",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine image",
      "set-by-server": false,
      "optional": true,
      "type": "Domain",
      "uri":"/api/project/$currentProject$/imageinstance.json",
      "uri-print-attribute":"instanceFilename",
      "uri-sort-attribute":"instanceFilename"
    },
    {
      "id": "offset",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Filename offset",
      "set-by-server": false,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "box_size",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Detection box size",
      "set-by-server": false,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "training_type",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "AI training type",
      "set-by-server": false,
      "optional": false,
      "type": "String"
    }, 
    {
      "id": "output_file",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "AI training output file",
      "set-by-server": false,
      "optional": false,
      "type": "String"
    }
  ],

  "schema-version": "cytomine-0.1"
}
